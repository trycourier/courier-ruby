module Courier
  module Models
    type email_header =
      { logo: Courier::Logo, bar_color: String?, inherit_default: bool? }

    class EmailHeader < Courier::Internal::Type::BaseModel
      attr_accessor logo: Courier::Logo

      attr_accessor bar_color: String?

      attr_accessor inherit_default: bool?

      def initialize: (
        logo: Courier::Logo,
        ?bar_color: String?,
        ?inherit_default: bool?
      ) -> void

      def to_hash: -> {
        logo: Courier::Logo,
        bar_color: String?,
        inherit_default: bool?
      }
    end
  end
end
